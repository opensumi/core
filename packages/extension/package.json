    "@opensumi/ide-decoration": "2.17.4",
    "@opensumi/ide-dev-tool": "^1.3.1",
    "@opensumi/ide-editor": "2.17.4",
    "@opensumi/ide-extension-storage": "2.17.4",
    "@opensumi/ide-file-tree-next": "2.17.4",
