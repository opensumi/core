{
  "name": "@opensumi/ide-extension",
  "version": "2.17.4",
  "files": [
    "lib",
    "hosted"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run build:pkg && npm run build:ext-host && npm run compile:worker",
    "build:pkg": "tsc --build ../../configs/ts/references/tsconfig.extension.json",
    "compile:worker": "webpack --config webpack.config.worker.js",
    "watch:worker": "webpack -w --config webpack.config.worker.js",
    "build:ext-host": "tsc -p ../../configs/ts/references/tsconfig.extension.json",
    "watch:ext-host": "tsc --watch -p ../../configs/ts/references/tsconfig.extension.json"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:opensumi/core.git"
  },
  "dependencies": {
    "@opensumi/ide-connection": "2.17.4",
    "@opensumi/ide-core-node": "2.17.4",
    "@opensumi/ide-debug": "2.17.4",
    "@opensumi/ide-file-search": "2.17.4",
    "@opensumi/ide-file-service": "2.17.4",
    "@opensumi/ide-logs": "2.17.4",
    "@opensumi/ide-static-resource": "2.17.4",
    "@opensumi/ide-task": "2.17.4",
    "@opensumi/ide-terminal-next": "2.17.4",
    "@opensumi/ide-webview": "2.17.4",
    "@opensumi/vscode-jsonrpc": "^8.0.0-next.2",
    "address": "^1.1.2",
    "glob-to-regexp": "0.4.1",
    "is-running": "^2.1.0",
    "picomatch": "^2.2.1",
    "tree-kill": "^1.2.2",
    "v8-inspect-profiler": "^0.0.20"
  },
  "devDependencies": {
    "@opensumi/ide-comments": "2.17.4",
    "@opensumi/ide-components": "2.17.4",
    "@opensumi/ide-core-browser": "2.17.4",
