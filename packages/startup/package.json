{
  "name": "@opensumi/ide-startup",
  "version": "2.19.9",
  "files": [
    "lib"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "start:lite": "webpack-dev-server --config ./webpack.lite.config.js",
    "start": "run-p start:client:open start:server start:webview",
    "start:webview": "webpack-dev-server --config ./webpack.webview.js",
    "start:client": "webpack-dev-server --config ./webpack.config.js",
    "start:server": "cross-env IS_DEV=1 NODE_ENV=development EXT_MODE=js KTLOG_SHOW_DEBUG=1 nodemon --inspect=9999 ./entry/web/server.ts",
    "start:no-inspect:client": "webpack-dev-server --config ./webpack.config.js",
    "start:no-inspect:server": "cross-env EXT_MODE=js KTLOG_SHOW_DEBUG=1 nodemon ./entry/web/server.ts",
    "start:client:open": "cross-env SUMI_DEV_OPEN_BROWSER=1 webpack-dev-server --config ./webpack.config.js",
    "start:server:extjs": "cross-env EXT_MODE=js NODE_ENV=development KTLOG_SHOW_DEBUG=1 node --inspect=9999 -r ts-node/register ./entry/web/server.ts",
    "build": "tsc --build ../../configs/ts/references/tsconfig.startup.json",
    "bundle:standard": "cross-env NODE_ENV=production webpack --config webpack.standard.config.js",
    "bundle:standard:analysis": "cross-env analysis=1 npm run bundle:standard",
    "bundle:lite": "cross-env NODE_ENV=production webpack --config webpack.lite.config.js",
    "bundle:lite:stats": "cross-env NODE_ENV=production webpack --profile --config webpack.lite.config.js --json > stats.json",
    "bundle:lite:analysis": "cross-env analysis=1 npm run bundle:lite"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:opensumi/core.git"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "**/lib/**",
      "**/node_modules/**",
      "**/__tests__/**",
      "**/src/browser/**",
      "**/core-browser/**",
      "**/extensions/common/**",
      "**/theme/**",
      "**/src/hosted/**",
      "**/*.d.ts"
    ],
    "verbose": true,
    "execMap": {
      "ts": "node --require ts-node/register"
    },
    "watch": [
      "../../packages"
    ],
    "events": {
      "crash": "kill -9 $(lsof -t -i:8000 -sTCP:LISTEN)"
    },
    "signal": "SIGTERM",
    "ext": "js,json,ts",
    "delay": "2000"
  },
  "dependencies": {
    "@opensumi/ide-addons": "2.19.9",
    "@opensumi/ide-comments": "2.19.9",
    "@opensumi/ide-core-browser": "2.19.9",
    "@opensumi/ide-core-common": "2.19.9",
    "@opensumi/ide-core-node": "2.19.9",
    "@opensumi/ide-debug": "2.19.9",
    "@opensumi/ide-decoration": "2.19.9",
    "@opensumi/ide-editor": "2.19.9",
    "@opensumi/ide-explorer": "2.19.9",
    "@opensumi/ide-extension": "2.19.9",
    "@opensumi/ide-extension-manager": "2.19.9",
    "@opensumi/ide-extension-storage": "2.19.9",
    "@opensumi/ide-file-scheme": "2.19.9",
    "@opensumi/ide-file-search": "2.19.9",
    "@opensumi/ide-file-service": "2.19.9",
    "@opensumi/ide-file-tree-next": "2.19.9",
    "@opensumi/ide-i18n": "2.19.9",
    "@opensumi/ide-keymaps": "2.19.9",
    "@opensumi/ide-logs": "2.19.9",
    "@opensumi/ide-main-layout": "2.19.9",
    "@opensumi/ide-markdown": "2.19.9",
    "@opensumi/ide-markers": "2.19.9",
    "@opensumi/ide-menu-bar": "2.19.9",
    "@opensumi/ide-monaco": "2.19.9",
    "@opensumi/ide-monaco-enhance": "2.19.9",
    "@opensumi/ide-opened-editor": "2.19.9",
    "@opensumi/ide-outline": "2.19.9",
    "@opensumi/ide-output": "2.19.9",
    "@opensumi/ide-overlay": "2.19.9",
    "@opensumi/ide-preferences": "2.19.9",
    "@opensumi/ide-process": "2.19.9",
    "@opensumi/ide-quick-open": "2.19.9",
    "@opensumi/ide-remote-opener": "2.19.9",
    "@opensumi/ide-scm": "2.19.9",
    "@opensumi/ide-search": "2.19.9",
    "@opensumi/ide-static-resource": "2.19.9",
    "@opensumi/ide-status-bar": "2.19.9",
    "@opensumi/ide-storage": "2.19.9",
    "@opensumi/ide-task": "2.19.9",
    "@opensumi/ide-terminal-next": "2.19.9",
    "@opensumi/ide-testing": "2.19.9",
    "@opensumi/ide-theme": "2.19.9",
    "@opensumi/ide-toolbar": "2.19.9",
    "@opensumi/ide-userstorage": "2.19.9",
    "@opensumi/ide-variable": "2.19.9",
    "@opensumi/ide-webview": "2.19.9",
    "@opensumi/ide-workspace": "2.19.9",
    "@opensumi/ide-workspace-edit": "2.19.9",
    "@opensumi/textmate-languages": "^2.6.1"
  },
  "devDependencies": {
    "@opensumi/ide-dev-tool": "^1.3.1",
    "browserfs": "^1.4.3",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "css-loader": "^2.1.1",
    "electron": "^18.3.0",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^1.0.4",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "less": "^3.9.0",
    "less-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "mobx": "^5.9.4",
    "mobx-react-lite": "^1.3.1",
    "nodemon": "^2.0.15",
    "npm-run": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "react-dom": "^16.8.6",
    "serviceworker-webpack-plugin": "^1.0.1",
    "style-loader": "^0.23.1",
    "style-resources-loader": "^1.2.1",
    "ts-loader": "^6.0.1",
    "ts-node": "^10.0.0",
