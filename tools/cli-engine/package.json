    "@ali/ide-startup": "1.8.20",
    "@koa/cors": "^3.0.0",
    "chalk": "^4.0.0",
    "ejs": "^2.7.4",
    "koa": "^2.7.0",
    "koa-mount": "^4.0.0",
    "open": "^7.0.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "yargs-parser": "^18.1.2"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.2",
