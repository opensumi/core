{
  "name": "@ali/kaitian-cli-engine",
  "version": "1.8.20",
  "description": "Integration engine runtime for kaitian-cli and kaitian extension",
  "main": "",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "run-p build:node build:browser build:exthost build:webview build:ext-worker-host",
    "build1": "npm run build:node && npm run build:browser && npm run build:exthost && npm run build:webview && npm run build:ext-worker-host",
    "build:browser": "webpack --config ./configs/webpack.config.browser.js --progress",
    "build:ext-worker-host": "webpack --config ./configs/webpack.config.ext-worker.js --progress",
    "build:ext-worker-host:watch": "npm run build:ext-worker-host -- --watch",
    "build:exthost": "webpack --config ./configs/webpack.config.ext.js --progress",
    "build:node": "tsc -p configs/tsconfig.node.json || echo 0",
    "build:webview": "webpack --config ./configs/webpack.config.webview.js --progress",
    "lint": "eslint --cache --ext .js ./",
    "lint:fix": "tnpm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "watch": "run-p watch:node watch:browser watch:exthost watch:webview watch:ext-worker-host",
    "watch:browser": "npm run build:browser -- --watch",
    "watch:exthost": "npm run build:exthost -- --watch",
    "watch:node": "tsc -w -p configs/tsconfig.node.json || echo 0",
    "watch:webview": "npm run build:webview -- --watch"
  },
  "dependencies": {
    "@ali/common-di": "^5.0.0",
    "@ali/ide-core-browser": "1.8.20",
    "@ali/ide-core-common": "1.8.20",
