{
  "name": "@opensumi/ide-electron",
  "version": "1.1.0",
  "description": "IDE Electron 实践层",
  "main": "./app/dist/main/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "link-local": "node ./scripts/link-local.js",
    "build:browser": "webpack --config ./build/webpack.browser.config.js ",
    "build:node": "webpack --config ./build/webpack.node.config.js ",
    "build:extension": "webpack --config ./build/webpack.extension-host.config.js ",
    "build:main": "webpack --config ./build/webpack.main.config.js ",
    "build:webview": "webpack --config ./build/webpack.webview.config.js ",
    "build-prod:browser": "webpack --config ./build/webpack.browser.config.js --mode=production",
    "build-prod:node": "webpack --config ./build/webpack.node.config.js --mode=production",
    "build-prod:extension": "webpack --config ./build/webpack.extension-host.config.js --mode=production",
    "build-prod:main": "webpack --config ./build/webpack.main.config.js --mode=production",
    "build-prod:webview": "webpack --config ./build/webpack.webview.config.js --mode=production",
    "watch:browser": "webpack --config ./build/webpack.browser.config.js -w --mode=development",
    "watch:node": "webpack --config ./build/webpack.node.config.js -w --mode=development",
    "watch:extension": "webpack --config ./build/webpack.extension-host.config.js -w --mode=development",
    "watch:main": "webpack --config ./build/webpack.main.config.js -w --mode=development",
    "watch:webview": "webpack --config ./build/webpack.webview.config.js -w --mode=development",
    "build": "rimraf -rf ./out && run-p build:node build:browser build:main build:extension build:webview",
    "build-prod": "rimraf -rf ./out && run-p build-prod:node build-prod:browser build-prod:main build-prod:extension build-prod:webview",
    "pack": "npm run build && node build/pack.js",
    "rebuild-native": "node ./scripts/rebuild-native.js --target=electron",
    "start": "electron --inspect=9229 .",
    "start:no-dev": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "git@github.com.com:ide-framework/ide-electron.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "getmac": "^1.4.6",
    "node-gyp": "^8.3.0",
    "node-pty": "0.10.0",
    "nsfw": "^2.1.2",
    "spdlog": "^0.9.0",
    "vscode-oniguruma": "1.5.1",
    "@opensumi/vscode-ripgrep": "^1.4.0",
    "yargs": "^13.2.4"
  },
  "resolutions": {
    "canvas": "2.6.1",
    "y18n": "4.0.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "electron": "13.6.2",
    "electron-builder": "^21.2.0",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^8.1.0",
    "glob-to-regexp": "0.4.1",
    "html-webpack-plugin": "^3.2.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "less": "^3.9.0",
    "less-loader": "^6.0.0",
    "md5": "^2.2.1",
    "mini-css-extract-plugin": "^0.6.0",
    "mobx": "^5.9.4",
    "mobx-react-lite": "^1.3.1",
    "npm-run": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "react-dom": "^16.8.6",
    "rimraf": "^3.0.0",
    "style-loader": "^0.23.1",
    "style-resources-loader": "^1.2.1",
    "ts-loader": "^6.0.1",
    "ts-node": "8.0.2",
    "tsconfig-paths": "^3.8.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^4.4.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "yargs": "^13.2.4"
  }
}
